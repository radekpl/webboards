# Deploy to gea (using appId & version from /earl/src/main/webapp/WEB-INF/appengine-web.xml) 
mvn gwt:compile appengine:upload

#Clean
mvn gwt:clean clean 

#First local run
mvn gwt:clean gwt:compile compile war:inplace gwt:debug

 
#DevMode
mvn gwt:run

#Super Dev Mode:
mvn gwt:run-codeserver
# On first run add bookmarklets from http://localhost:9876/
mvn gwt:clean gwt:compile compile war:inplace gwt:debug
google-chrome http://127.0.0.1:8888
# On source change press "Dev Mode On" bookmarklet while on http://127.0.0.1:8888/bastogne/?table=... page


#After changes to Serializable classes that are sent to client you need to restart gwt:run