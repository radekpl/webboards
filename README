# Deploy to gea (using appId & version from /earl/src/main/webapp/WEB-INF/appengine-web.xml) 
mvn gwt:compile appengine:upload

#Clean
mvn gwt:clean clean 

#First local run
mvn gwt:clean gwt:compile compile war:inplace gwt:run
mvn gwt:clean gwt:compile compile war:inplace gwt:debug

 

#DevMode
mvn gwt:run	